public class Distribuidora {

    public static void main(String[] args) {
        double valorTotal = 181759.98; // valor total mensal da distribuidora
        double sp = 67836.43; 
        double rj = 36678.66; 
        double mg = 29229.88; 
        double es = 27165.48; 
        double outros = 19849.53; 
        
        
        double percentualSP = (sp / valorTotal) * 100;
        double percentualRJ = (rj / valorTotal) * 100;
        double percentualMG = (mg / valorTotal) * 100;
        double percentualES = (es / valorTotal) * 100;
        double percentualOutros = (outros / valorTotal) * 100;
        
        // exibe os resultados
        System.out.printf("Percentual de São Paulo:  percentualSP);
        System.out.printf("Percentual do Rio de Janeiro:  percentualRJ);
        System.out.printf("Percentual de Minas Gerais:  percentualMG);
        System.out.printf("Percentual do Espírito Santo:  percentualES);
        System.out.printf("Percentual dos outros estados:  percentualOutros);
    }

}


//Percentual de São Paulo: 37,28%
//Percentual do Rio de Janeiro: 20,16%
//Percentual de Minas Gerais: 16,06%
//Percentual do Espírito Santo: 14,92%
//Percentual dos outros estados: 10,58%
